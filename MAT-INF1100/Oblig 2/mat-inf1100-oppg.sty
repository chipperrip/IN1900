%
% Customized Theoremstyle
%
\newtheoremstyle{LMtheorem}% name
  {3pt}%      Space above
  {3pt}%      Space below
  {\slshape}%         Body font
  {}%         Indent amount (empty = no indent, \parindent = para indent)
  {\bfseries}% Thm head font
  {.}%        Punctuation after thm head
  {.5em}%     Space after thm head: " " = normal interword space;
        %       \newline = linebreak
  {}%         Thm head spec (can be left empty, meaning `normal')

\newtheoremstyle{LMexample}% name
  {4pt}%      Space above
  {4pt}%      Space below
  {\normalfont\fontsize{9}{10pt}\selectfont}%         Body font
  {}%         Indent amount (empty = no indent, \parindent = para indent)
  {\bfseries\normalsize}% Thm head font
  {.}%        Punctuation after thm head
  {.5em}%     Space after thm head: " " = normal interword space;
        %       \newline = linebreak
  {}%         Thm head spec (can be left empty, meaning `normal')

\theoremstyle{LMtheorem}
\newtheorem{theorem}{Teorem}
\newtheorem{proposition}[theorem]{Proposition}
