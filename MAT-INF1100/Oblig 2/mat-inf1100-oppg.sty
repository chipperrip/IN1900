%
% Alternative sectioning commands.
%
%  The arguments of \verb=\@startsection= are given for reference:
% % #1    #2     #3      #4          #5         #6
% {NAME}{LEVEL}{INDENT}{BEFORESKIP}{AFTERSKIP}{STYLE}
%
%
\def\section{\@startsection{section}%
        1\z@{-12\p@\@plus-9\p@}{6\p@}%
        {\centering\normalfont\bfseries\fontsize{12}{14pt}\selectfont}}
\def\subsection{\@startsection{subsection}%
        2\z@{-9\p@\@plus-6\p@}{4\p@}%
        {\centering\normalfont\bfseries}}

\def\subsubsection{\@startsection{subsubsection}%
        3\z@{9\p@\@plus6\p@}{-.5em}%
        \rmdefault\itshape}

% Style for captions.
%
\long\def\@makecaption#1#2{%
\setbox\@tempboxa\vbox{\rm\fontsize{8}{10pt}\selectfont%
    {\bf#1}\expandafter\@ifnotempty\expandafter
        {\@cdr#2\@nil}{.\upshape\enspace#2}%
%    \unskip\lparindent\par
    \unskip\par
     \global\setbox\@ne\lastbox}%
\ifhbox\@ne % the normal case
  \setbox\@ne\hbox{\unhbox\@ne\unskip\unskip\unpenalty}%
\fi
  \ifdim\wd\@tempboxa=\z@ % this means caption will fit on one line
    \setbox\@ne\hbox to\columnwidth{\hss\box\@ne\hss}%
  \else % tempboxa contained more than one line (or other vertical mode stuff)
    \setbox\@ne\vbox{\unvbox\@tempboxa\parskip\z@skip
        \noindent\unhbox\@ne\par}%
\fi
  \ifnum\@tempcnta<64 % if the float IS a figure...
    \vskip 1pc%
\box\@ne
  \else % if the float IS NOT a figure...
    \box\@ne
    \vskip 1pc%
  \fi
\relax
}

